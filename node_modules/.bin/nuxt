#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/data/data/com.termux/files/home/3DM-Mod-Comment/node_modules/.pnpm/nuxt@3.15.4_@parcel+watcher@2.5.1_@types+node@22.12.0_db0@0.2.3_eslint@8.57.1_ioredis@5_55fd7895796c14f8b86b6ecc17c40b52/node_modules/nuxt/bin/node_modules:/data/data/com.termux/files/home/3DM-Mod-Comment/node_modules/.pnpm/nuxt@3.15.4_@parcel+watcher@2.5.1_@types+node@22.12.0_db0@0.2.3_eslint@8.57.1_ioredis@5_55fd7895796c14f8b86b6ecc17c40b52/node_modules/nuxt/node_modules:/data/data/com.termux/files/home/3DM-Mod-Comment/node_modules/.pnpm/nuxt@3.15.4_@parcel+watcher@2.5.1_@types+node@22.12.0_db0@0.2.3_eslint@8.57.1_ioredis@5_55fd7895796c14f8b86b6ecc17c40b52/node_modules:/data/data/com.termux/files/home/3DM-Mod-Comment/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/data/data/com.termux/files/home/3DM-Mod-Comment/node_modules/.pnpm/nuxt@3.15.4_@parcel+watcher@2.5.1_@types+node@22.12.0_db0@0.2.3_eslint@8.57.1_ioredis@5_55fd7895796c14f8b86b6ecc17c40b52/node_modules/nuxt/bin/node_modules:/data/data/com.termux/files/home/3DM-Mod-Comment/node_modules/.pnpm/nuxt@3.15.4_@parcel+watcher@2.5.1_@types+node@22.12.0_db0@0.2.3_eslint@8.57.1_ioredis@5_55fd7895796c14f8b86b6ecc17c40b52/node_modules/nuxt/node_modules:/data/data/com.termux/files/home/3DM-Mod-Comment/node_modules/.pnpm/nuxt@3.15.4_@parcel+watcher@2.5.1_@types+node@22.12.0_db0@0.2.3_eslint@8.57.1_ioredis@5_55fd7895796c14f8b86b6ecc17c40b52/node_modules:/data/data/com.termux/files/home/3DM-Mod-Comment/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nuxt/bin/nuxt.mjs" "$@"
else
  exec node  "$basedir/../nuxt/bin/nuxt.mjs" "$@"
fi
